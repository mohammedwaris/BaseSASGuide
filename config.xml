<?xml version='1.0' encoding='utf-8'?>
<widget xmlns="http://www.w3.org/ns/widgets" 
	xmlns:gap   = "http://phonegap.com/ns/1.0"
	id="com.jlojic.basesasguide" 
	version="1.0.0" 
	versionCode = "100">
	
    <name>Base SAS Exam Guide</name>
    <description>
        A Practical guide for Base SAS certification.
    </description>
    <author email="jlojic.apps@gmail.com" href="http://www.jlojic.com">
        JLojic Team
    </author>
	
    <preference name="phonegap-version" value="3.5.0" /> 
    <preference name="orienation" value="default" />
    <preference name="exit-on-suspend" value="true" />
    <feature name="http://api.phonegap.com/1.0/network"/>
	
    <content src="index.html" />
    <access origin="*" />
	<icon src="icon.png"/>
	
	<gap:plugin name="com.leonid.smartmobilesoftware.inappbilling" version="3.0.0">
		<param name="billing_key" value=""/>
	</gap:plugin>
	
    <!-- Android -->
    <gap:platform name="android" />
    <preference name="android-minSdkVersion" value="8" />
    <preference name="android-targetSdkVersion" value="18" /> 
    
    <icon src="icons/android/icon36x36.png" gap:platform="android" gap:density="ldpi" />
    <icon src="icons/android/icon48x48.png" gap:platform="android" gap:density="mdpi" />
    <icon src="icons/android/icon72x72.png" gap:platform="android" gap:density="hdpi" />
    <icon src="icons/android/icon96x96.png" gap:platform="android" gap:density="xhdpi" />
	
	
	
	
	
</widget>
